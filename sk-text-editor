<script src="https://cdn.ckeditor.com/ckeditor5/35.1.0/classic/ckeditor.js"></script>


<div id="editor">This is some sample content.</div>
<script>
    ClassicEditor
.create(document.querySelector('#editor')).then(editor => {
    console.log(editor);
}).catch(error => {
    console.error(error);
});
</script>
