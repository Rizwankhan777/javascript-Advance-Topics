//link
https://stackoverflow.com/questions/32453253/jquery-for-change-dropdown-icon-box-after-clicked

//code
// dropdown icon change on click
$(".btn_body").click(function () {
    $(this).find('i').toggleClass('fa-angle-up fa-angle-down');
    if ($(".btn_body").not(this).find("i").hasClass("fa-angle-down")) {
        $(".btn_body").not(this).find("i").toggleClass('fa-angle-up fa-angle-down');
    }
});
